{
    "name": "Castlingo",
    "version": "0.0.1",
    "permissions": [
        "tabs"
    ],
    "manifest_version": 3,
    "web_accessible_resources": [
    {
      "resources": ["index.html"],
      "matches": ["<all_urls>"]
    }
  ],
    "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["inject.js"]
    }
  ],
  "host_permissions": ["<all_urls>"],
    "action": {
        "default_icon": "icons/duolingo.png",
        "default_popup": "index.html"
    }
}